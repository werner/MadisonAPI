User json sql=users
    email String
    password String
    confirmationToken String Maybe
    confirmationTokenExpiration UTCTime Maybe
    createdAt UTCTime Maybe
    updatedAt UTCTime Maybe
    UniqueUserEmail email
    deriving Show

Session json sql=sessions
    userId  UserId
    cookie  String
    deriving Show

Product json sql=products
    code String Maybe
    name String
    description Text Maybe
    productType Int
    userId UserId
    createdAt UTCTime Maybe
    updatedAt UTCTime Maybe
    deriving Show

Warehouse json sql=warehouses
    name String
    userId UserId
    scopedId Int
    UniqueWarehouseNameUserId name userId
    createdAt UTCTime Maybe
    updatedAt UTCTime Maybe
    deriving Show

Stock json sql=stocks
    warehouseId WarehouseId
    productId ProductId
    amount Double
    createdAt UTCTime Maybe
    updatedAt UTCTime Maybe
    deriving Show
