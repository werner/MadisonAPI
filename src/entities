User json sql=users
    email String
    password String
    createdAt UTCTime Maybe
    updatedAt UTCTime Maybe
    deriving Show

Product json sql=products
    code String Maybe
    name String
    description Text Maybe
    product_type Int
    userId UserId
    createdAt UTCTime Maybe
    updatedAt UTCTime Maybe
    deriving Show

Warehouse json sql=warehouses
    name String
    userId UserId
    createdAt UTCTime Maybe
    updatedAt UTCTime Maybe
    deriving Show

Stock json sql=stocks
    warehouseId WarehouseId
    productId ProductId
    amount Double
    createdAt UTCTime Maybe
    updatedAt UTCTime Maybe
    deriving Show
